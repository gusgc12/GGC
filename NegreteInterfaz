    private void igualActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_igualActionPerformed
        // TODO add your handling code here:
        Negrete calcula = new Negrete(consola.getText());
        
        string = new StringBuilder();
        
        if(calcula.revisaParéntesis()){
            String s = calcula.convierteAPostfija();
            String p = calcula.evaluaOperación(s);
            consola.setText(p);
        }
        else {
            consola.setText("Error en paréntesis");
        }
        
    }//GEN-LAST:event_igualActionPerformed

